{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "identity-api-route": {
        "ClusterId": "identity-api",
        "Match": {
          "Path": "/api/v{version}/Auth/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identity-api": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "identity-api-1": {
            "Address": "https://soluciondesde0-api-identity"
          },
          "identity-api-2": {
            "Address": "https://soluciondesde0-api-identity2"
          }
        }
      }
    }
  }
}