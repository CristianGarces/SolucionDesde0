{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "Issuer": "SolucionDesde0Db.API.Identity",
    "Audience": "apigateway",
    "ExpireMinutes": 600
  },
  "ReverseProxy": {
    "Routes": {
      "identity-api-route": {
        "ClusterId": "identity-api",
        "RateLimiterPolicy": "open",
        "Match": {
          "Path": "/api/v{version}/Auth/{**catch-all}"
        }
      },
      "identity-api-route-users": {
        "ClusterId": "identity-api",
        "RateLimiterPolicy": "open",
        "Match": {
          "Path": "/api/v{version}/Users/{**catch-all}"
        }
      },
      "identity-api-route-usermangment": {
        "ClusterId": "identity-api",
        "RateLimiterPolicy": "open",
        "Match": {
          "Path": "/api/v{version}/UserManagement/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identity-api": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "identity-api-1": {
            "Address": "https+http://soluciondesde0-api-identity"
          } //,
          //"identity-api-2": {
          //  "address": "https://soluciondesde0-api-identity2"
          //}
        }
      }
    }
  }
}